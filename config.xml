<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.schedule" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ScheduleEvent</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <icon src="res/logo.png" />
    <content src="login.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="AndroidExtraFilesystems" value="sdcard,cache" />
    <engine name="android" spec="^6.2.3" />
    <plugin name="com.dbaq.cordova.contactsPhoneNumbers" spec="~0.0.9" />
    <plugin name="cordova-plugin-file" spec="^4.3.3" />
    <plugin name="cordova-plugin-googlemaps" spec="^1.4.3">
        <variable name="API_KEY_FOR_ANDROID" value="AIzaSyANmqhLtdTYacJPsYj0e2c020pPddzb-7s" />
        <variable name="API_KEY_FOR_IOS" value="AIzaSyANmqhLtdTYacJPsYj0e2c020pPddzb-7s" />
    </plugin>
    <plugin name="cordova-plugin-image-picker" spec="^1.1.3" />
    <plugin name="cordova-plugin-native-keyboard" spec="^1.4.1">
        <variable name="LICENSE" value="TRIAL" />
    </plugin>
    <plugin name="cordova-plugin-spinner-dialog" spec="^1.3.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
